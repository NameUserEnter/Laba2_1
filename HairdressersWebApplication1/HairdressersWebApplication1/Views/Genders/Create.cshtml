@model HairdressersWebApplication1.Gender

@{
    ViewData["Title"] = "Create";
}

<h1>Створити</h1>

<h4>Гендер</h4>
<hr />
<div class="text-danger">@ViewBag.F</div>
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Gender1" class="control-label"></label>
                <input asp-for="Gender1" class="form-control" />
                <span asp-validation-for="Gender1" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Створити" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Назад до списку</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
